{
  "name": "@faithconnect/meetings-service",
  "version": "1.0.0",
  "description": "Meetings Service - Live Prayer Meetings with Agora",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:fast": "esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --watch --external:express --external:agora-access-token --external:mysql2 --external:pg --external:redis --external:jsonwebtoken --external:zod --external:winston --external:socket.io",
    "build": "tsc",
    "build:fast": "esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --external:express --external:agora-access-token --external:mysql2 --external:pg --external:redis --external:jsonwebtoken --external:zod --external:winston --external:socket.io",
    "start": "node dist/index.js",
    "test": "jest",
    "test:websocket": "node scripts/test-websocket-events.js",
    "test:websocket:integration": "node scripts/test-websocket-integration.js",
    "test:recording:integration": "node scripts/test-recording-integration.js",
    "test:resources:integration": "node scripts/test-resource-integration.js",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "agora-token": "^2.0.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "prom-client": "^15.1.2",
    "redis": "^4.6.11",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "esbuild": "^0.19.12",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.3",
    "socket.io-client": "^4.8.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
